{
  "expo": {
    "name": "DrinkStore",
    "slug": "DrinkStore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/product.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/product.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thinh0311.drinkstore"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.thinh0311.drinkstore"
    },
    "web": {}
  }
}
